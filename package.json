{
  "homepage": ".",
  "name": "react-typescript-starter-pack",
  "version": "0.1.0",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@vitejs/plugin-react": "^4.3.4",
    "bulma": "^1.0.3",
    "classnames": "^2.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "5.0.1",
    "react-transition-group": "^4.4.5",
    "typescript": "^4.6.3",
    "typescript-eslint-parser": "^22.0.0",
    "vite": "^6.0.11",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "npm run format && eslint ./src --fix",
    "start": "vite",
    "build": "vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "latest",
    "@mate-academy/students-ts-config": "latest",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "eslint": "^9.19.0",
    "gh-pages": "^6.3.0",
    "sass": "^1.83.4",
    "prettier": "^3.4.2"
  }
